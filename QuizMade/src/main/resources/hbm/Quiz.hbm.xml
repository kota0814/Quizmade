<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.example.quizmade.domain">
 <class name="Quiz" table="quiz"
 dynamic-insert="true" dynamic-update="true">
 <id name="id" column="id">
 <generator class="native" />
 </id>
 <property name="question" column="question" type="string" />
 <property name="answer" column="answer" type="string" />
 <property name="choices1" column="choices1" type="string" />
 <property name="choices2" column="choices2" type="string" />
 <property name="choices3" column="choices3" type="string" />
 </class>
</hibernate-mapping>